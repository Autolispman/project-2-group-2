<section class="section section-stats center">
    <div class="row">
        <div class="col 13 m11 s12">
            <div class="card-panel">
                <form class="Home" action="" method="">
                    <div class="form-group">
                        <label for="street_address1" >Street Address</label>
                        <input type="text" class="form-control" id="street_address1" placeholder="" name="">
                    </div>
                    <div class="form-group">
                        <label for="street_address2" >Street Address(2)</label>
                        <input type="text" class="form-control" id="street_address2" placeholder="" name="">
                    </div>
                    <div class="form-group">
                        <label for="city" >City</label>
                        <input type="text" class="form-control" id="city" placeholder="" name="">
                    </div>
                    <div class="form-group">
                        <label for="state" >State</label>
                        <input type="text" class="form-control" id="state" placeholder="TX" name="">
                    </div>
                    <div class="form-group">
                        <label for="zip" >Zip</label>
                        <input type="number" class="form-control" id="zip" placeholder="" name="">
                    </div>
                    <div class="form-group">
                        <label for="emergContFirstName" >Emergency First</label>
                        <input type="text" class="form-control" id="emergContFirstName" placeholder="" name="">
                    </div>
                    <div class="form-group">
                        <label for="emergContLastName" >Emergency Last</label>
                        <input type="text" class="form-control" id="emergContLastName" placeholder="" name="">
                    </div>
                    <div class="form-group">
                        <label for="emergContPhone" >Emergency Phone</label>
                        <input type="text" class="form-control" id="emergContPhone" placeholder="" name="">
                    </div>
                    <div>
                        <label for="gateCode" > Gate Entry</label>
                        <input type="text" class="form-control" id="gateCode" placeholder="" name="">
                    </div>
                    <div>
                        <label for="doorCode" >Door Entry</label>
                        <input type="text" class="form-control" id="doorCode" placeholder="" name="">
                    </div>
                    <div>
                        <label for="alarmCode" >Alarm Passcode</label>
                        <input type="text" class="form-control" id="alarmCode" placeholder="" name="">
                    </div>
                    <div>
                        <label for="keyInstructions" >Key Instructions</label>
                        <input type="text" class="form-control" id="keyInstructions" placeholder="" name="">
                    </div>
                    <div>
                        <label for="wifiPassword" >Wifi Password</label>
                        <input type="text" class="form-control" id="wifiPassword" placeholder="" name="">
                    </div>
                    <div>
                        <input id="user_id" name="user_id" type="hidden" value="{{this.id}}">
                    </div>
                    <button id="submitHome" class="btn btn-primary float-right">Submit</button>
                </form>
            </div>
        </div>
    </div>
<script type="text/javascript" src="js/homeForm.js"></script>
</section>